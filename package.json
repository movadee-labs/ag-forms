{
  "name": "ag-forms",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "npm": "Use Yarn instead of NPM",
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "postinstall": "npm run decorate-angular-cli && npm run ngcc",
    "affected:build": "nx affected:build --configuration=production",
    "affected:ci": "yarn affected:build && yarn affected:lint && yarn affected:test && yarn affected:e2e --configuration=ci-production",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint --max-warnings=0",
    "affected:test": "nx affected:test",
    "build": "nx build --configuration=production",
    "ci": "yarn build && yarn lint && yarn test && yarn e2e --configuration=ci-production",
    "decorate-angular-cli": "node ./decorate-angular-cli.js",
    "e2e": "nx e2e example-e2e",
    "lint": "nx run-many --target=lint --all --max-warnings=0",
    "lint-fix": "nx run-many --target=lint --all --fix",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx run-many --target=test --all  --skip-nx-cache --parallel --coverage --coverageReporters=lcov && node ./coverage.util.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.0",
    "@nrwl/angular": "14.8.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@ngneat/spectator": "^11.2.1",
    "@nrwl/cli": "14.8.2",
    "@nrwl/cypress": "14.8.2",
    "@nrwl/eslint-plugin-nx": "14.8.2",
    "@nrwl/jest": "14.8.2",
    "@nrwl/linter": "14.8.2",
    "@nrwl/nx-cloud": "14.7.0",
    "@nrwl/workspace": "14.8.2",
    "@theunderscorer/nx-semantic-release": "^1.14.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "autoprefixer": "^10.4.0",
    "cypress": "^10.7.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "glob": "^8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.2",
    "ng-mocks": "^14.2.3",
    "ng-packagr": "~14.2.0",
    "nx": "14.8.2",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}

