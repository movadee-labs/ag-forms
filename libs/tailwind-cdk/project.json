{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/tailwind-cdk/src",
  "prefix": "ag-forms",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/tailwind-cdk"],
      "options": {
        "project": "libs/tailwind-cdk/ng-package.json",
        "tailwindConfig": "libs/tailwind-cdk/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/tailwind-cdk/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/tailwind-cdk/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/tailwind-cdk"],
      "options": {
        "jestConfig": "libs/tailwind-cdk/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/tailwind-cdk/**/*.ts",
          "libs/tailwind-cdk/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
